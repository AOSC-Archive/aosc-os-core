PKGNAME=mpfr
PKGSEC=libs
PKGDEP="gmp glibc"
PKGDES="Functions for multiple precision math"

# The following configuration:
#
# 1. Declares documentation directory (in a different position);
# 2. Enables threaded safety;

AUTOTOOLS_AFTER="--docdir=/usr/share/doc/mpfr --enable-thread-safe"

AB_FLAGS_O3=1
AB_FLAGS_SPECS=0
