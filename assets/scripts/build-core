for i in \
	core-devel/linux+api \
	core-misc/tzdata \
	core-libs/glibc \
	core-libs/ncurses \
	core-libs/readline \
	core-libs/zlib \
	core-libs/gmp \
	core-libs/mpfr \
	core-libs/mpc \
	core-devel/gcc \
	core-devel/binutils \
	core-devel/libtool \
	core-libs/gdbm \
	core-database/db \
	core-perl/perl \
	core-shells/bash \
	core-devel/make \
	core-misc/aosc-aaa
do
	echo "->	Starting build of $i\n" &&
	abbs-build $i &&
	echo "->	Finished build of $i\n"
done
